{
  "template": {
    "schema_name": "com.sec.android.app.sbrowser.beta.settings",
    "query_template_name": "search"
  },
  "query": {
    "text": "$0",
    "bool": {
      "should": [
        {
          "wildcard": {
            "fields": [
              "title",
              "title_eng"
            ],
            "term_operator": "and",
            "text": "$0"
          }
        }
      ]
    }
  },
  "fallback_queries": [
    {
      "text": "$0",
      "bool": {
        "should": [
          {
            "chosung": {
              "fields": [
                "title"
              ],
              "chosung_field": "title_chosung",
              "term_operator": "and"
            }
          },
          {
            "phonetic": {
              "fields": [
                "title_phonetic",
                "title_eng_phonetic"
              ],
              "term_operator": "and"
            }
          },
          {
            "fuzzy": {
              "fields": [
                "title_fuzzy",
                "title_eng_fuzzy"
              ],
              "minimum_length": 4,
              "fuzziness": 1,
              "term_operator": "and"
            }
          }
        ]
      }
    }
  ],
  "sort": {
    "id_sort": "desc"
  }
}

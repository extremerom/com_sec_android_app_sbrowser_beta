{
  "template": {
    "schema_name": "com.sec.android.app.sbrowser.beta.sites",
    "query_template_name": "search"
  },
  "query": {
    "text": "$0",
    "bool": {
      "must": [
        {
          "match": {
            "fields": [
              "page"
            ],
            "text": "$1"
          }
        },
        {
          "match": {
            "fields": [
              "private"
            ],
            "text": "$2"
          }
        },
        {
          "wildcard": {
            "fields": [
              "title",
              "url"
            ],
            "term_operator": "and",
            "text": "$0"
          }
        }
      ]
    }
  },
  "fallback_queries": [
    {
      "text": "$0",
      "bool": {
        "must": [
          {
            "match": {
              "fields": [
                "page"
              ],
              "text": "$1"
            }
          },
          {
            "match": {
              "fields": [
                "private"
              ],
              "text": "$2"
            }
          },
          {
            "bool": {
              "should": [
                {
                  "chosung": {
                    "fields": [
                      "title"
                    ],
                    "chosung_field": "title_chosung",
                    "term_operator": "and"
                  }
                },
                {
                  "phonetic": {
                    "fields": [
                      "title_phonetic"
                    ],
                    "term_operator": "and"
                  }
                },
                {
                  "fuzzy": {
                    "fields": [
                      "title_fuzzy",
                      "url_fuzzy"
                    ],
                    "minimum_length": 4,
                    "fuzziness": 1,
                    "term_operator": "and"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ],
  "sort": {
    "id_num": "desc"
  }
}

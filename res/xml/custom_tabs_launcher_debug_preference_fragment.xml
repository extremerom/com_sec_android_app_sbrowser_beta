    <ListPreference android:entries="@array/custom_tabs_launcher_debug_settings_close_button_position_entries" android:title="@string/pref_custom_tabs_launcher_close_button_position" android:key="pref_custom_tabs_launcher_close_button_position" android:summary="%s" android:defaultValue="0" android:entryValues="@array/custom_tabs_launcher_debug_settings_close_button_position_values" />

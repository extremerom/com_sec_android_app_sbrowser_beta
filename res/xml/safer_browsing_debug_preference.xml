<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:sbrowser="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory android:title="@string/debug_settings_title" android:selectable="false">
        <com.sec.android.app.sbrowser.settings.debug.DebugSwitchPreference android:title="@string/safer_browsing_debug_enable" sbrowser:debugDefaultValue="false" sbrowser:debugKey="pref_safer_browsing_debug_enable" />
        <Preference android:title="@string/safer_browsing_debug_configuration" android:key="pref_safer_browsing_debug_configuration" android:fragment="com.sec.android.app.sbrowser.settings.debug.SaferBrowsingDebugConfigurationPreferenceFragment" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/safer_browsing_debug_database" android:selectable="false">
        <Preference android:title="@string/safer_browsing_debug_threat_descriptors" android:key="pref_threat_descriptors_status" android:fragment="com.sec.android.app.sbrowser.settings.debug.SaferBrowsingDebugDatabaseFragment" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/safer_browsing_debug_server" android:selectable="false">

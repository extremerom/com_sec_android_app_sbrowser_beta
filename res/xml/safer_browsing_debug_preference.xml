        <com.sec.android.app.sbrowser.settings.debug.DebugSwitchPreference android:title="@string/safer_browsing_debug_log_enable" sbrowser:debugDefaultValue="false" sbrowser:debugKey="pref_safer_browsing_debug_log_enable" />
        <Preference android:title="@string/safer_browsing_debug_threat_list_updates" android:key="pref_fetch_threat_list_updates" />
        <com.sec.android.app.sbrowser.settings.debug.DebugSwitchPreference android:title="@string/safer_browsing_debug_minimum_wait_duration_enable" sbrowser:debugDefaultValue="false" sbrowser:debugKey="pref_safer_browsing_debug_minimum_wait_duration_enable" />
    </PreferenceCategory>
</PreferenceScreen>
